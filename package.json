{
	"name": "@dassana-io/web-utils",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/dassana-io"
	},
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com:dassana-io/web-utils",
		"directory": "@dassana-io/web-utils"
	},
	"version": "0.10.1",
	"dependencies": {
		"axios": "^0.27.2",
		"axios-retry": "^3.3.1",
		"bytes": "^3.1.2",
		"can-ndjson-stream": "^1.0.2",
		"date-fns": "^2.28.0",
		"date-fns-tz": "^1.3.5",
		"he": "^1.2.0",
		"json-merge-patch": "^1.0.2",
		"json2csv": "^5.0.7",
		"jsonpath-plus": "^7.0.0",
		"lodash": "^4.17.21",
		"mitt": "^2.1.0",
		"pluralize": "^8.0.0",
		"query-string": "^7.1.1",
		"react-scripts": "^5.0.1",
		"uuid": "^8.3.2"
	},
	"scripts": {
		"api-models:generate": "rm -rf src/api/$npm_config_output_folder; mkdir -p src/api; cp -R $npm_config_input_folder/model src/api/$npm_config_output_folder; rm -rf $npm_config_input_folder; cd src/api/$npm_config_output_folder; mv models.ts index.ts; prettier --write .",
		"api-models:global": "npm run api-models:generate --input_folder=globalApi --output_folder=global",
		"api-models:profile": "npm run api-models:generate --input_folder=profileApi --output_folder=profile",
		"codegen": "node codegen.js",
		"build": "rollup -c rollup.config.ts",
		"test": "react-scripts test",
		"test-coverage": "npm test -- --coverage --watchAll=false",
		"lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
		"eject": "react-scripts eject"
	},
	"main": "lib/index.cjs.js",
	"module": "lib/index.esm.js",
	"types": "lib/types",
	"jest": {
		"clearMocks": true,
		"coveragePathIgnorePatterns": [
			"src/api/*/",
			"src/constants.ts"
		],
		"resetMocks": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^22.0.1",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@testing-library/react": "^13.3.0",
		"@types/bytes": "^3.1.1",
		"@types/he": "^1.1.2",
		"@types/jest": "^28.1.4",
		"@types/json-merge-patch": "^0.0.8",
		"@types/json2csv": "^5.0.3",
		"@types/lodash": "^4.14.182",
		"@types/node": "^18.0.3",
		"@types/pluralize": "^0.0.29",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.30.5",
		"@typescript-eslint/parser": "^5.30.5",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"jest-watch-typeahead": "^1.1.0",
		"prettier": "^2.7.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-test-renderer": "^18.2.0",
		"rollup": "^2.75.7",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.32.1",
		"ts-jest": "^28.0.5",
		"typescript": "^4.7.4"
	},
	"peerDependencies": {
		"react": "^17.0.2 || ^18",
		"react-dom": "^17.0.2 || ^18"
	}
}
